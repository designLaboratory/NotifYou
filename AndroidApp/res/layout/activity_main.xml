<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="esapp.com.notifyoulight.MainActivity">

    <TextView
        android:text="Phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="17dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="15dp"
        android:id="@+id/textView6" />

    <Button
        android:text="-"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/b_phone_m"
        style="@style/Widget.AppCompat.Button.Small"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="42dp"
        android:layout_marginEnd="42dp" />

    <Button
        android:text="+"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/b_phone_p"
        style="@style/Widget.AppCompat.Button.Small"
        android:layout_alignBaseline="@+id/textView6"
        android:layout_alignBottom="@+id/textView6"
        android:layout_toLeftOf="@+id/t_sms_cnt"
        android:layout_toStartOf="@+id/t_sms_cnt"
        android:layout_marginRight="22dp"
        android:layout_marginEnd="22dp" />

    <TextView
        android:text="SMS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView9"
        android:layout_below="@+id/b_phone_m"
        android:layout_alignLeft="@+id/textView6"
        android:layout_alignStart="@+id/textView6"
        android:layout_marginTop="20dp" />

    <Button
        android:text="-"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/b_sms_m"
        style="@style/Widget.AppCompat.Button.Small"
        android:layout_alignBaseline="@+id/b_sms_p"
        android:layout_alignBottom="@+id/b_sms_p"
        android:layout_alignLeft="@+id/b_phone_m"
        android:layout_alignStart="@+id/b_phone_m" />

    <TextView
        android:text="0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/t_sms_cnt"
        android:layout_alignBaseline="@+id/b_sms_p"
        android:layout_alignBottom="@+id/b_sms_p"
        android:layout_alignLeft="@+id/t_phone_cnt"
        android:layout_alignStart="@+id/t_phone_cnt" />

    <Button
        android:text="-"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/b_mail_m"
        style="@style/Widget.AppCompat.Button.Small"
        android:layout_below="@+id/b_sms_m"
        android:layout_alignLeft="@+id/b_sms_m"
        android:layout_alignStart="@+id/b_sms_m" />

    <Button
        android:text="+"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/b_mail_p"
        style="@style/Widget.AppCompat.Button.Small"
        android:layout_below="@+id/b_sms_p"
        android:layout_alignLeft="@+id/b_sms_p"
        android:layout_alignStart="@+id/b_sms_p" />

    <Button
        android:text="+"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/b_sms_p"
        style="@style/Widget.AppCompat.Button.Small"
        android:layout_below="@+id/b_phone_m"
        android:layout_alignLeft="@+id/b_phone_p"
        android:layout_alignStart="@+id/b_phone_p" />

    <Button
        android:text="+"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/b_mess_p"
        style="@style/Widget.AppCompat.Button.Small"
        android:layout_below="@+id/b_mail_p"
        android:layout_alignLeft="@+id/b_mail_p"
        android:layout_alignStart="@+id/b_mail_p" />

    <TextView
        android:text="Mail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView11"
        android:layout_alignBaseline="@+id/b_mail_p"
        android:layout_alignBottom="@+id/b_mail_p"
        android:layout_alignLeft="@+id/textView9"
        android:layout_alignStart="@+id/textView9" />

    <Button
        android:text="-"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/b_mess_m"
        style="@style/Widget.AppCompat.Button.Small"
        android:layout_below="@+id/b_mail_m"
        android:layout_alignLeft="@+id/b_mail_m"
        android:layout_alignStart="@+id/b_mail_m" />

    <TextView
        android:text="0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/t_mail_cnt"
        android:layout_alignBaseline="@+id/b_mail_p"
        android:layout_alignBottom="@+id/b_mail_p"
        android:layout_alignLeft="@+id/t_sms_cnt"
        android:layout_alignStart="@+id/t_sms_cnt" />

    <Button
        android:text="+"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/b_fb_p"
        style="@style/Widget.AppCompat.Button.Small"
        android:layout_below="@+id/b_mess_p"
        android:layout_alignLeft="@+id/b_mess_p"
        android:layout_alignStart="@+id/b_mess_p" />

    <TextView
        android:text="Messenger"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView15"
        android:layout_alignBaseline="@+id/b_mess_p"
        android:layout_alignBottom="@+id/b_mess_p"
        android:layout_alignLeft="@+id/textView11"
        android:layout_alignStart="@+id/textView11" />

    <Button
        android:text="-"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/b_fb_m"
        style="@style/Widget.AppCompat.Button.Small"
        android:layout_below="@+id/b_mess_m"
        android:layout_alignLeft="@+id/b_mess_m"
        android:layout_alignStart="@+id/b_mess_m" />

    <TextView
        android:text="0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/t_mess_cnt"
        android:layout_alignBaseline="@+id/b_mess_p"
        android:layout_alignBottom="@+id/b_mess_p"
        android:layout_alignLeft="@+id/t_mail_cnt"
        android:layout_alignStart="@+id/t_mail_cnt" />

    <TextView
        android:text="0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/t_fb_cnt"
        android:layout_alignBaseline="@+id/b_fb_p"
        android:layout_alignBottom="@+id/b_fb_p"
        android:layout_alignLeft="@+id/t_mess_cnt"
        android:layout_alignStart="@+id/t_mess_cnt" />

    <TextView
        android:text="Facebook"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView17"
        android:layout_alignBaseline="@+id/b_fb_p"
        android:layout_alignBottom="@+id/b_fb_p"
        android:layout_alignLeft="@+id/textView15"
        android:layout_alignStart="@+id/textView15" />

    <TextView
        android:text="0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/t_phone_cnt"
        android:layout_marginRight="26dp"
        android:layout_marginEnd="26dp"
        android:layout_alignBaseline="@+id/b_phone_p"
        android:layout_alignBottom="@+id/b_phone_p"
        android:layout_toLeftOf="@+id/b_phone_m"
        android:layout_toStartOf="@+id/b_phone_m" />

    <Button
        android:text="Start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/b_fb_p"
        android:layout_toStartOf="@+id/b_fb_p"
        android:layout_marginBottom="22dp"
        android:id="@+id/b_start" />

    <Button
        android:text="RESET"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/b_start"
        android:layout_alignRight="@+id/b_fb_m"
        android:layout_alignEnd="@+id/b_fb_m"
        android:layout_marginRight="26dp"
        android:layout_marginEnd="26dp"
        android:id="@+id/b_reset" />
</RelativeLayout>
